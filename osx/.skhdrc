# Reload yabai
ctrl + alt + cmd - r : launchctl kickstart -k "gui/${UID}/homebrew.mxcl.yabai"

# Application Shortcuts
ctrl + shift - n : open ~
ctrl - return : open -n /Applications/Alacritty.app
ctrl + shift - return : open -n /Applications/Google\ Chrome.app

ctrl - left : yabai -m space --focus prev
ctrl - right : yabai -m space --focus next
ctrl - q : yabai -m space --focus 1
ctrl - w : yabai -m space --focus 2
ctrl - e : yabai -m space --focus 3
ctrl - r : yabai -m space --focus 4
ctrl - a : yabai -m space --focus 5
ctrl - s : yabai -m space --focus 6
ctrl - d : yabai -m space --focus 7
ctrl - f : yabai -m space --focus 8

# Move windows to certain spaces
ctrl + shift - q : yabai -m window --space  1; yabai -m space --focus 1
ctrl + shift - w : yabai -m window --space  2; yabai -m space --focus 2
ctrl + shift - e : yabai -m window --space  3; yabai -m space --focus 3
ctrl + shift - r : yabai -m window --space  4; yabai -m space --focus 4
ctrl + shift - a : yabai -m window --space  5; yabai -m space --focus 5
ctrl + shift - s : yabai -m window --space  6; yabai -m space --focus 6
ctrl + shift - d : yabai -m window --space  7; yabai -m space --focus 7
ctrl + shift - f : yabai -m window --space  8; yabai -m space --focus 8